{
   "background": {
      "page": "html/proxy.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "19": "resources/imgs/icon19.png",
         "38": "resources/imgs/icon38.png"
      },
      "default_popup": "/html/popup/popup.html",
      "default_title": "Find hidden photos"
   },
   "content_scripts": [ {
      "js": [ "/vendor/oauth2/oauth2_inject.js" ],
      "matches": [ "http://www.bing.com/robots.txt*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "read_inject.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "View hidden photos of anyone on the social media (This is a NON-official Facebook app)",
   "icons": {
      "128": "resources/imgs/icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "PictureMate - View hidden FB pics",
   "permissions": [ "notifications", "tabs", "storage", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking" ],
   "short_name": "PictureMate",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.3",
   "web_accessible_resources": [ "/vendor/oauth2/oauth2.html" ]
}
